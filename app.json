{
  "expo": {
    "name": "Teleconsult",
    "scheme": "teleconsult",
    "slug": "teleconsult-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gtc-app-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gtc-app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.harrypotter23.teleconsult"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gtc-app-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gtclogo.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f1f9beb2-a625-454a-857c-0b84dda1e330"
      }
    }
  }
}
